{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "./dist/",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    //"noEmit": true,
    "jsx": "react-jsx",
    //  "outDir": "./dist/",
    //   "noImplicitAny": true,
    //   "module": "es6",
    //   "target": "es5",
    //   "jsx": "react",
    //   "allowJs": true,
    //   "allowSyntheticDefaultImports": true,
    //   "moduleResolution": "node",
    //   "sourceMap": true,
    //   //"strict": true,
    //   "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}